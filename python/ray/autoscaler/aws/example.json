{
    "cluster_name": "default",
    "min_workers": 0,
    "max_workers": 2,
    "provider": {
        "type": "aws",
        "region": "us-east-1"
    },
    "auth": {
        "ssh_user": "ubuntu"
    },
    "head_node": {
        "InstanceType": "p2.xlarge",
        "ImageId": "ami-212d465b"
    },
    "node": {
        "InstanceType": "m4.large",
        "ImageId": "ami-212d465b"
    },
    "file_mounts": {
        "/tmp/data": "/home/eric/Desktop/data"
    },
    "head_init_commands": [
        "cd ~/ray; git remote add eric https://github.com/ericl/ray.git || true",
        "cd ~/ray; git fetch eric && git reset --hard 4b57344c",
        "~/.local/bin/ray stop",
        "~/.local/bin/ray start --head --redis-port=6379 --autoscaling-config=~/ray_bootstrap_config.json"
    ],
    "init_commands": [
        "~/.local/bin/ray stop",
        "~/.local/bin/ray start --redis-address=$RAY_HEAD_IP:6379"
    ]
}
